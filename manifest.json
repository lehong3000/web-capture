{
	"name": "AutoCapture",
	"description": "Tự động capture hình ảnh gửi lên server",
	"version": "1.1",
	"manifest_version": 3,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [ "https://*/*", "http://*/*" ],
			"js": [ "jquery.min.js", "main.js" ]
		}
	],
	"icons": {
		"16": "/images/vin_16.png",
		"32": "/images/vin_32.png",
		"48": "/images/vin_48.png",
		"128": "/images/vin_128.png"
	}
}
